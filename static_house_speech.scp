ON=Place trashcan*
	src.f_static_place_trashcan

ON=Remove trashcan*
	ref1=<room.tag0.sign>
	if !((<src.is_static_owner <ref1>>) || (<src.f_static_iscoowner <ref1>>) || (<src.f_static_isfriend <ref1>>) || (<src.isgm>))
		return 0
	else
		if (<ref1.tag0.trashcan>)
			try uid.<ref1.tag0.trashcan>.remove
			ref1.tag.trashcan =
			src.sysmessage @,,1 The trash can has been removed.
		else
			src.sysmessage @33,,1 You have no trashcan to remove.
		endif
		return 1
	endif

ON=commands*
	src.f_resenddialog d_static_hs_commands

