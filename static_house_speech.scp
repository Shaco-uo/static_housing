ON=bank*
	if <def.hs_can_bank>
		ref1=<room.tag0.sign>
		if !((<src.is_static_owner <ref1>>) || (<src.f_static_iscoowner <ref1>>) || (<src.f_static_isfriend <ref1>>) || (<src.isgm>))
			return 0
		else
			src.bankself
			src.sysmessage @,,1 You have <f_math_cat <src.bankbalance>> gold in your Bank Box.
			return 1
		endif
	endif

