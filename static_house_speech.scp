VERSION=X1

[SPEECH spk_static_cmds]
ON=lock this*
	ref1=<room.tag0.sign>
	if !((<src.is_static_owner <ref1>>) || (<src.f_static_iscoowner <ref1>>) || (<src.f_static_isfriend <ref1>>) || (<src.isgm>))
		return 0
	else
		src.targetf f_static_item_lock
		src.message @,,1 What do you wish to lockdown?
		return 1
	endif

